<odoo>
    <data>
        <record id="s_license_view_form" model="ir.ui.view">
            <field name="name">s_license_view_form</field>
            <field name="model">s.license</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title">
                            <label for="s_license" string="License"/>
                            <h1>
                                <field name="s_license" placeholder="e.g. Odoo Inc."/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="s_license_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
    </data>
</odoo>